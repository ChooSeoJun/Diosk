<Window x:Class="Diosk.PaymentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diosk"
        mc:Ignorable="d"
        Title="PaymentWindow" Height="450" Width="800" Background="Transparent">
    <Window.Resources>
        <Style x:Key="TextStyle" TargetType="TextBlock">
            <Setter Property="FontWeight" Value="Bold"/>
            <Setter Property="FontSize" Value="15"/>
        </Style>
    </Window.Resources>

    <Grid>
        <Rectangle RadiusX="15" RadiusY="15" Fill="White" Width="450"></Rectangle>
        <TextBlock x:Name="foodOrderList" Style="{StaticResource TextStyle}" Margin="250,20,250,100"/>
    </Grid>
</Window> 
