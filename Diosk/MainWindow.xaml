<Window x:Class="Diosk.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diosk"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Width="100" Height="100" Click="Button_Click"></Button>
        <local:Statistic x:Name="navi" Visibility="Collapsed"/>

        <Button Width="100" Height="50" Click="Order_Click" VerticalAlignment="Bottom"
                HorizontalAlignment="Left" Margin="10" Content="주문"></Button>
    </Grid>
</Window>
